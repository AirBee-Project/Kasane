{
  "$schema": "./schema.json",
  "user": "default",
  "commands": [
    {
      "AddSpace": {
        "spacename": "space1"
      }
    },
    {
      "AddKeys": {
        "spacename": "space1",
        "key": [
          {
            "name": "temperature",
            "type": "Int"
          },
          {
            "name": "active",
            "type": "Boolean"
          }
        ]
      }
    },
    {
      "PutValue": {
        "select": {
          "SpaceTimeIdSet": {
            "inner": [
              {
                "z": 8,
                "f": {
                  "Any": {}
                },
                "x": {
                  "Single": 128
                },
                "y": {
                  "Single": 256
                },
                "i": 0,
                "t": "Any"
              },
              {
                "z": 8,
                "f": {
                  "AfterUnLimitRange": 0
                },
                "x": {
                  "BeforeUnLimitRange": 255
                },
                "y": {
                  "LimitRange": [
                    0,
                    511
                  ]
                },
                "i": 30,
                "t": {
                  "Single": 4
                }
              }
            ]
          }
        },
        "keys": [
          {
            "name": "temperature"
          },
          {
            "name": "active"
          }
        ]
      }
    }
  ]
}